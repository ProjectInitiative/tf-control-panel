root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true

[*.rs]
trim_trailing_whitespace = false

[*.md]
trim_trailing_whitespace = false
max_line_length = 72

[*.toml]
indent_size = 2
trim_trailing_whitespace = false
